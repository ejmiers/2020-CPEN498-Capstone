<launch>
	<node name="joystick_controller" pkg="rosserial_python" type="serial_node.py" output="screen">
		<param name="port" value="/dev/ttyACM0"/>
		<param name="baud" value="57600"/>
	</node>
	<node name="servo_handler" pkg="rosserial_python" type="serial_node.py" output="screen">
		<param name="port" value="/dev/ttyACM1"/>
		<param name="baud" value="57600"/>
	</node>
	<node name="main_node" pkg="main_node" type="controller.py" output="screen" />
</launch>
